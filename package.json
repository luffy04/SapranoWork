    "brace": "^0.11.1",
    "chart.js": "^2.9.4",
    "compile-run": "^2.3.2",
    "concurrently": "^3.5.1",
    "express": "^4.16.2",
    "express-session": "^1.17.0",
    "fs": "0.0.1-security",
    "mongodb": "^3.6.3",
    "mysql": "^2.18.1",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "react-ace": "^8.0.0",
    "react-ace-editor": "0.0.3",
    "react-chartjs-2": "^2.11.1",
    "react-icons": "^3.11.0",
    "react-images-upload": "^1.2.8",
